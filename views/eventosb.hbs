<body class="fondo1">
    <!-- Navbar -->
    <header id="header" class="fixed-top header-transparent">
      <div class="container-fluid">
        <div class="row justify-content-center">
          <div class="col-xl-11 d-flex align-items-center">
            <a href="/" class="mr-auto"><img src="img/nav-tinyy.png" style="float:left; margin-left:-40px;"></a>
            <nav class="nav-menu d-none d-lg-block">
              <ul>
                <li class="active"><a href="/">inicio</a></li>
                <li><a href="/eventos">Eventos</a></li>
                <li class="drop-down"><a href="/articulos">Articulos</a>
                  <ul>
                    <li><a href="/camisas">Camisas</a></li>
                    <li><a href="/termos">Termos</a></li>
                    <li><a href="/invitaciones">Invitaciones</a></li>
                    <li><a href="/globos">Globos</a></li>
                  </ul>
                </li>
                {{#if logueado}}
                <li class="drop-down"><a href="">{{ name }}</a>
                  <ul>
                    <li><a href="/cerrar">Cerrar sesion</a></li>
                  </ul>
                </li>

                {{else}}
                <button type="button" class="btn btn-outline-info my-2 my-sm-0 letras" data-toggle="modal" data-target="#ModalCenter" style="margin-left: 5px;">Iniciar Sesión</button>
                {{/if}}
              </ul>
            </nav>
          </div>
        </div>
      </div>
    </header>
    <div class="modal fade" id="ModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
          <div class="modal-dialog modal-dialog-centered" role="document">
              <div class="modal-content">
                  <div class="modal-header">
                      <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                          <span aria-hidden="true">&times;</span>
                      </button>
                  </div>
                  <div class="modal-body">
                      <form method="POST" action="/login">

                          <div class="modal-body">
                              <img class="img" src="imagenes/avatarx2.png" alt="">
                              <h2 class="letras">Iniciar sesion</h2>
                              <div class="form-group row">
                                  <input class="form-control form-control-lg formularios" name="correo" type="email" placeholder="Correo">
                              </div>
                              <div class="form-group row">
                                  <input class="form-control form-control-lg formularios" name="pass" type="password" placeholder="Contraseña">
                              </div>

                          </div>
                          <div class="modal-footer">
                              <button type="submit" class="btn btn-info my-2 btn-block boton" style="font-family: Alesandra;">Continuar</button>
                          </div>
                      </form>
                      {{#if message }}
                          <h4 class="alert alert-danger mt-3" style="text-align: center;">{{message}}</h4>
                      {{/if}}
                  </div>
              </div>
          </div>
      </div>


<br>
